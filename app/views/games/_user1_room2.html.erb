<div class="d-flex justify-content-center align-items-center h-100 position-relative", style= "width:1000px", data-controller="door-opening game-subscription"
  data-game-subscription-game-id-value="<%= @game.id %>"
  data-door-opening-advance-path-value="<%= advance_game_path(@game) %>"
  data-action="mousemove->mooving-effect-object#moovingEffect"
  data-controller="mooving-effect-object"
>
  <div class="marble-floor-room2" data-mooving-effect-object-target="floor"></div>
  <div>
    <%= image_tag "porte_4.png", class: "door-img w-100 ", height: "310", "data-action": "click->door-opening#deactivateDoorTrap" %>
    <%= image_tag "carpet_3a.png", class: "carpet-img position-absolute", height: "80" %>
    <%= image_tag "bombonne_1.png", class: "bombonne-img position-absolute", height: "90" %>
    <%= image_tag "chemine_1.png", class: "chemine-img position-absolute", height: "430" %>
    <%= image_tag "bureau_1.png", class: "bureau-img position-absolute", height: "200" %>
    <%= image_tag "livre_2.png", class: "livre-img position-absolute", height: "70" %>
    <%= image_tag "bougie_2.png", class: "bougie-img position-absolute", height: "30" %>
    <%= image_tag "piledelivre_2.png", class: "pile-img position-absolute", height: "270" %>
    <%= image_tag "piledelivre_1.png", class: "pile-img1 position-absolute", height: "180" %>
    <%= image_tag "table_2.png", class: "table-img position-absolute", height: "150" %>
    <%= image_tag "lustre_2.jpg", class: "lustre-img position-absolute", height: "150" %>
    <%= image_tag "clef_1.png", class: "clef-img position-absolute", height: "10", "data-action": "click->door-opening#grabKey", "data-door-opening-good-key-param": "false" %>
    <%= image_tag "clef_2.png", class: "clef2-img position-absolute", height: "25", "data-action": "click->door-opening#grabKey", "data-door-opening-good-key-param": "true" %>
    <%= image_tag "clef_3.png", class: "clef3-img position-absolute", height: "20", "data-action": "click->door-opening#grabKey", "data-door-opening-good-key-param": "false" %>
  </div>

  <div  class="position-absolute"
    data-controller="door-opening game-subscription"
    data-game-subscription-game-id-value="<%= @game.id %>"
    data-door-opening-advance-path-value="<%= advance_game_path(@game) %>"
  >

  </div>
</div>
